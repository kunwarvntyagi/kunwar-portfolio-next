{
    "trailingSlash": "auto",
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "400": {
        "rewrite": "/404.html"
      },
      "403": {
        "rewrite": "/404.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    },
    "globalHeaders": {
      "strict-transport-security": "max-age=15724800; includeSubdomains"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }